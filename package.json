{
  "name": "boilermaker",
  "version": "2.0.0",
  "description": "Some boilerplate code to get you started - get shakin'!",
  "main": "index.js",
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "postinstall": "touch secrets.js",
    "seed": "node script/seed.js",
    "start": "node server",
    "start-dev": "NODE_ENV='development' npm run build-client-watch & NODE_ENV='development' JWT=shh npm run start-server",
    "start-server": "nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "NODE_ENV='test' mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" \"./script/**/*.spec.js\" --require @babel/register --require esm ",
    "test:dev": "npm run test -- --watch"
  },
  "author": "Fullstack Academy of Code",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "history": "^4.9.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "pg": "^8.5.1",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "axios": "^0.21.1",
    "axios-mock-adapter": "^1.16.0",
    "babel-loader": "^8.2.2",
    "chai": "^4.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "esm": "^3.2.25",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.1",
    "react-router-dom": "^5.0.0",
    "react-test-renderer": "^16.4.2",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "supertest": "^4.0.2",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0"
  }
}
